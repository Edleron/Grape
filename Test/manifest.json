{
    "manifest_version": 3,
    "name": "Sayfa Yeniden Aç",
    "version": "1.0",
    "description": "Bulunduğunuz web sayfasını yeniden açar.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }